{
  "name": "aztec-mock-pocs",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test:noir": "cd ./src/noir_test && ../../../../noir/noir-repo/target/release/nargo test --oracle-resolver 'http://localhost:5555/'",
    "test:jest": "DEBUG='aztec:*' NODE_NO_WARNINGS=1 node --experimental-vm-modules $(yarn bin jest) --runInBand --testTimeout=60000 --forceExit",
    "run:resolver": "nodemon ./src/server.ts",
    "codegen:mock": "node ../cli/dest/bin/index.js codegen ./src/mocks/target/MockTestedContract-MeaningOfLife.json -o ./src/artifacts --ts",
    "compile:mock": "cd ./src/mocks && ../../../../noir/noir-repo/target/release/nargo compile"
  },
  "dependencies": {
    "@aztec/accounts": "workspace:^",
    "@aztec/aztec.js": "workspace:^",
    "@aztec/simulator": "workspace:^",
    "@jest/globals": "^29.5.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.26",
    "express": "^4.18.3",
    "jest": "^29.7.0",
    "json-rpc-2.0": "^1.7.0",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0"
  },
  "nodemonConfig": {
    "watch": [
      "server.ts",
      "sandbox.ts"
    ],
    "ext": "ts",
    "execMap": {
      "ts": "ts-node-esm"
    }
  }
}
