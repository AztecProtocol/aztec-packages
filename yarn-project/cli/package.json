{
  "name": "@aztec/cli",
  "version": "0.32.0",
  "type": "module",
  "exports": {
    "./contracts": "./dest/cmds/contracts/index.js",
    "./devnet": "./dest/cmds/devnet/index.js",
    "./infrastructure": "./dest/cmds/infrastructure/index.js",
    "./l1": "./dest/cmds/l1/index.js",
    "./pxe": "./dest/cmds/pxe/index.js",
    "./cli-utils": "./dest/utils/index.js",
    "./misc": "./dest/cmds/misc/index.js",
    "./setup-contracts": "./dest/cmds/misc/setup_contracts.js",
    "./utils": "./dest/utils/index.js",
    "./inspect": "./dest/utils/inspect.js"
  },
  "typedocOptions": {
    "entryPoints": [
      "./dest/builder/index.js"
    ],
    "name": "Aztec CLI",
    "tsconfig": "./tsconfig.json"
  },
  "scripts": {
    "build": "yarn clean && tsc -b",
    "build:dev": "tsc -b --watch",
    "clean": "rm -rf ./dest .tsbuildinfo",
    "test": "NODE_NO_WARNINGS=1 node --experimental-vm-modules ../node_modules/.bin/jest --passWithNoTests --maxWorkers=${JEST_MAX_WORKERS:-8}"
  },
  "inherits": [
    "../package.common.json"
  ],
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.[cm]?js$": "$1"
    },
    "testRegex": "./src/.*\\.test\\.(js|mjs|ts)$",
    "rootDir": "./src",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {
      "^.+\\.tsx?$": [
        "@swc/jest",
        {
          "jsc": {
            "parser": {
              "syntax": "typescript",
              "decorators": true
            },
            "transform": {
              "decoratorVersion": "2022-03"
            }
          }
        }
      ]
    },
    "reporters": [
      "default"
    ],
    "testTimeout": 120000,
    "setupFiles": [
      "../../foundation/src/jest/setup.mjs"
    ],
    "testEnvironment": "../../foundation/src/jest/env.mjs"
  },
  "dependencies": {
    "@aztec/archiver": "workspace:^",
    "@aztec/aztec.js": "workspace:^",
    "@aztec/constants": "workspace:^",
    "@aztec/entrypoints": "workspace:^",
    "@aztec/foundation": "workspace:^",
    "@aztec/l1-artifacts": "workspace:^",
    "@aztec/p2p": "workspace:^",
    "@aztec/stdlib": "workspace:^",
    "@aztec/world-state": "workspace:^",
    "@iarna/toml": "^2.2.5",
    "@libp2p/peer-id-factory": "^3.0.4",
    "commander": "^12.1.0",
    "lodash.chunk": "^4.2.0",
    "lodash.groupby": "^4.6.0",
    "semver": "^7.5.4",
    "source-map-support": "^0.5.21",
    "tslib": "^2.4.0",
    "viem": "2.23.7"
  },
  "devDependencies": {
    "@aztec/accounts": "workspace:^",
    "@aztec/ethereum": "workspace:^",
    "@aztec/protocol-contracts": "workspace:^",
    "@jest/globals": "^30.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash.chunk": "^4.2.9",
    "@types/lodash.groupby": "^4.6.9",
    "@types/lodash.startcase": "^4.4.7",
    "@types/node": "^22.15.17",
    "@types/semver": "^7.5.2",
    "@types/source-map-support": "^0.5.10",
    "jest": "^30.0.0",
    "jest-mock-extended": "^3.0.5",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@aztec/accounts": "workspace:^",
    "@aztec/bb-prover": "workspace:^",
    "@aztec/ethereum": "workspace:^",
    "@aztec/l1-artifacts": "workspace:^",
    "@aztec/noir-contracts.js": "workspace:^",
    "@aztec/noir-protocol-circuits-types": "workspace:^",
    "@aztec/noir-test-contracts.js": "workspace:^",
    "@aztec/protocol-contracts": "workspace:^",
    "@aztec/stdlib": "workspace:^"
  },
  "files": [
    "dest",
    "src",
    "!*.test.*"
  ],
  "types": "./dest/index.d.ts",
  "engines": {
    "node": ">=20.10"
  }
}
