{
  "name": "@aztec/foundation",
  "version": "0.0.0",
  "packageManager": "yarn@3.4.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./dest/index.js",
    "./eslint": "./.eslintrc.cjs",
    "./prettier": "./.prettierrc.json"
  },
  "scripts": {
    "build": "yarn clean && tsc -b tsconfig.dest.json",
    "build:dev": "tsc -b tsconfig.dest.json --watch",
    "clean": "rm -rf ./dest .tsbuildinfo",
    "formatting": "prettier --check ./src && eslint --max-warnings 0 ./src",
    "test": "NODE_NO_WARNINGS=1 node --experimental-vm-modules $(yarn bin jest) --no-cache"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "testRegex": "./src/.*\\.test\\.ts$",
    "rootDir": "./src"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "debug": "^4.3.4",
    "detect-node": "^2.1.0"
  },
  "prettier": "./src/prettier-config/index.js",
  "yalcSig": "6fbabe5e1e3b5094c9842e160930d04a"
}
