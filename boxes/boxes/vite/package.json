{
  "name": "@aztec/vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "compile": "cd src/contracts && ${AZTEC_NARGO:-aztec-nargo} compile --silence-warnings",
    "codegen": "${AZTEC_BUILDER:-aztec} codegen src/contracts/target -o artifacts",
    "clean": "rm -rf ./dist .tsbuildinfo ./artifacts ./src/contracts/target",
    "prep": "yarn clean && yarn compile && yarn codegen",
    "build": "yarn prep && tsc -b && vite build",
    "serve": "vite",
    "test": "npx playwright test",
    "formatting": "prettier --check ./src && eslint ./src",
    "formatting:fix": "prettier -w ./src",
    "preview": "vite preview"
  },
  "dependencies": {
    "@aztec/accounts": "latest",
    "@aztec/aztec.js": "latest",
    "@aztec/bb-prover": "latest",
    "@aztec/circuit-types": "latest",
    "@aztec/key-store": "latest",
    "@aztec/kv-store": "latest",
    "@aztec/pxe": "latest",
    "@aztec/simulator": "latest",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-toastify": "^10.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^6.0.3",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-static-copy": "^2.2.0"
  }
}
