    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@playwright/test": "1.49.0",
    "@types/buffer-json": "^2",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "prettier": "^2.8.8",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.11.0",
